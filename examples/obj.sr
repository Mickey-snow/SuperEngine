
file = "bg47";
fn create_effects_obj(file){
  obj = Object();
  obj.init();
  obj.create(file, 1);

  x = obj.get_size_x();
  y = obj.get_size_y();
  obj.set_center_rep(x/2, y/2);

  obj.set_scale(1500,1500);

  return obj;
}

bg = create_effects_obj(file);

fn test_poseve(obj, delay = 100){
  obj.set_pos(-480, -270);
  obj.set_xeve(480, 35000, delay, 0);
  obj.set_yeve(-270, 35000, delay, 0);
}